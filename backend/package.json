{
  "name": "pg-takehome",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cd frontend && next dev",
    "build": "next build",
    "start": "npx ts-node -r dotenv/config src/server.ts",
    "lint": "next lint",
    "start-backend": "npx nodemon backend/server.ts",
    "codegen:gql": "graphql-codegen",
    "codegen:sql": "npx pgtyped -w -c config.json",
    "migration:run": "npx prisma migrate dev --schema=./backend/db/schema.prisma",
    "codegen": "graphql-codegen --config codegen.yaml"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@graphql-cli/codegen": "^3.0.4",
    "@pgtyped/cli": "^2.3.0",
    "@pgtyped/query": "^2.3.0",
    "@prisma/client": "^5.4.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "lodash": "^4.17.21",
    "next": "13.5.4",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@types/cors": "^2.8.14",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20",
    "@types/pg": "^8.6.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "postcss": "^8",
    "prisma": "^5.4.2",
    "tailwindcss": "^3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
